{
    "headers": [
      {
        "source" : "assets/*",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=31536000"
        }]
      }, {
        "source" : "index.html",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "no-cache"
        }]
      }
    ]
}
